
<% include ../partials/header %>


<header class="jumbotron">
    <div class="container">
            <h1>WELCOME to AI SONGS !!</h1>
            <p>View our handpicked SONGS from all over the world</p>
    </div> 
</header>

  <div class="row text-center" style="display:flex; flex-wrap:wrap">
        <% songs.forEach(function(song){ %>
            <div class="col-md-4 col-sm-6 col-lg-3">
                <div class="thumbnail">
                      <img src= "<%= song.cover_image %>" height="400px" width="400px" >
                      <div class="caption">
                            <h4><%= song.SONGS %></h4>
                      </div>
                      <p>
                          <a href="/songs/<%= song._id %>" class="btn btn-primary">ROCK THE MUSIC</a>
                      </p>
                </div>   
            </div>
                
            <% }); %>
  </div>




<% include ../partials/footer %>